{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        },
        "fileLoggingMode": "always",
        "logLevel": {
            "default": "Trace"
        }
    },
    "rabbitMQ": {
        "prefetchCount": 100,
        "queueName": "thresholds",
        "connectionString": "amqp://guest:guest@localhost:5672",
        "port": 5672
    }
}
