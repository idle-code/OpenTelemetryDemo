{
    "version": "2.0",
    "telemetryMode": "OpenTelemetry",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": false
            },
            "dependencyTrackingOptions": {
                "enableSqlCommandTextInstrumentation": false
            },
            "enableLiveMetrics": false,
            "enableDependencyTracking": false,
            "enablePerformanceCountersCollection": false,
            "httpAutoCollectionOptions": {
                "enableHttpTriggerExtendedInfoCollection": false,
                "enableW3CDistributedTracing": false,
                "enableResponseHeaderInjection": false
            },
            "snapshotConfiguration": {
                "isEnabled": false,
                "isEnabledInDeveloperMode": false,
                "provideAnonymousTelemetry": false,
            }
        },
        "fileLoggingMode": "always",
        "logLevel": {
            "default": "Trace"
        }
    }
}
